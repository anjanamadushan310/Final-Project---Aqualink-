# Production Environment Configuration
# Copy this to .env.production and update with your actual production values

# Backend API URL (REQUIRED)
# Replace with your deployed backend URL
VITE_API_URL=https://your-backend-domain.com/api

# Environment
NODE_ENV=production

# TinyMCE API Key (if using rich text editor)
VITE_TINYMCE_API_KEY=your_production_api_key_here

# Optional: Enable/disable debug logs
VITE_DEBUG=false
